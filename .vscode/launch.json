      {
        "name": "(gdb) Launch",
        "type": "cppdbg",
        "request": "launch",
        "program": "${fileDirname}\\receive.exe",
        "args": ["9000"],
        "stopAtEntry": false,
        "cwd": "${workspaceFolder}",
        "environment": [],
